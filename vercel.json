{
  "version": 2,
  "builds": [
    {
      "src": "dist-api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/dist-api/chat.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}