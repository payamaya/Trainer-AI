{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist"
      }
    },
    {
      "src": "src/api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/((.*))",
      "destination": "/src/api/$1"
    },
    {
      "src": "/assets/((.*))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true,
      "destination": "/dist/assets/$1"
    },
    {
      "src": "/site-dbrEskbr.webmanifest",
      "headers": {
        "Content-Type": "application/manifest+json"
      },
      "destination": "/dist/site-dbrEskbr.webmanifest"
    },
    {
      "src": "/((.*))",
      "destination": "/dist/index.html"
    }
  ]
}